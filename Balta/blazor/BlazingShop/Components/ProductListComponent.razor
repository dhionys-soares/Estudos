<h3>Products</h3>
<ul>
    @foreach (var item in _products)
    {
        <li>@item.Titulo</li>
    }
</ul>

@code {
    private List<Product> _products = new();
    protected override void OnInitialized()
    {
        for (int i = 0; i < 25; i++)
        {
            _products.Add(new Product
                {
                    Id = i,
                    Titulo = $"Produto {i}",
                    Price = i + 13.75M,
                    CategoryId = i,
                    Category = new Category()
                });
        }
    }
}
